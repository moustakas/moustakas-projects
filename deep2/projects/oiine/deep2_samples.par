# DEEP2 [OII] electron density sample definitions

typedef struct {
  char sample[];
  float zmin;
  float zmax;
  float faintcut;
  float brightcut;
} SAMPLE;

SAMPLE zbin1 0.75 0.90 -19.5 -23.0
